{
  "version": 2,
  "buildCommand": "echo 'No build required for static site'",
  "outputDirectory": "deploy",
  "builds": [
    {
      "src": "deploy/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/deploy/$1"
    },
    {
      "src": "/",
      "dest": "/deploy/index.html"
    }
  ]
}
